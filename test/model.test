UTest({
	'root model' () {
		var model = {
			name: 'Foo'
		};
		var div = mask.render("span > '~[name]'", model);
		var test = `
			text Foo;
			with (model) {
				name Foo
			}
		`;
		
		return DomTest(div, test, model);
	},
	
	'component model' () {
		mask.registerHandler(':foo', mask.Compo({
			model: {
				name: 'Baz'
			}
		}));
		
		var div = mask.render(":foo > span > '~[name]'");
		var test = `
			text Baz;
			with (model) {
				name Baz
			}
		`;
		
		return DomTest(div, test);
	}
})